<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="localCacheScope" value="STATEMENT"></setting>
		<setting name="jdbcTypeForNull" value="NULL" />  
	</settings>

	<typeAliases>
		<typeAlias alias="freeboardVO" type="kr.or.ddit.vo.FreeboardVO" />
		
		<!-- 재호  -->
		<typeAlias alias="friendVO" type="kr.or.ddit.vo.FriendVO"/>
		<typeAlias alias="chatingRoomVO" type="kr.or.ddit.vo.ChatingRoomVO"/>
		<typeAlias alias="messageVO" type="kr.or.ddit.vo.MessageVO"/>
		<typeAlias alias="participationVO" type="kr.or.ddit.vo.ParticipationVO"/>
		<typeAlias alias="dietVO" type="kr.or.ddit.vo.DietVO"/>
		
		<!-- 용춘 -->
		<typeAlias alias="mypillVO" type="kr.or.ddit.vo.MypillVO" />
		<typeAlias alias="pillVO" type="kr.or.ddit.vo.PillVO" />
		<typeAlias alias="mypillfileVO" type="kr.or.ddit.vo.MypillFileVO" />
		<!-- 한별 -->
		<typeAlias alias="scheduleVO" type="kr.or.ddit.vo.ScheduleVO" />
		<!-- 승민 -->	
		<typeAlias alias="memberVO" type="kr.or.ddit.vo.MemberVO" />
			
		<!-- 윤희 -->
		<typeAlias alias="boardVO" type="kr.or.ddit.vo.BoardVO" />
		
		<!-- 수아 -->
		<typeAlias alias="knowVO" type="kr.or.ddit.vo.KnowledgeVO" />
		<typeAlias alias="fileitemVO" type="kr.or.ddit.vo.FileItemVO"/>
		<typeAlias alias="healthFileVO" type="kr.or.ddit.vo.HealthFileVO"/>
		<typeAlias alias="healthImageVO" type="kr.or.ddit.vo.HealthImageVO"/>
		
	</typeAliases>

	<!-- 매퍼에는 얼리어스 줄수없 -->

	<mappers>
		<mapper resource="kr/or/ddit/mybatis/freeboard.xml" />
	<!-- 재호  -->
		<mapper resource="kr/or/ddit/mybatis/chat.xml" />
		<mapper resource="kr/or/ddit/mybatis/diet.xml" />
		
	<!-- 용춘 -->
		<mapper resource="kr/or/ddit/mybatis/medical.xml" />
		<mapper resource="kr/or/ddit/mybatis/medicalfile.xml" />
		<mapper resource="kr/or/ddit/mybatis/pill.xml" />
	<!-- 한별 -->
	    <mapper resource="kr/or/ddit/mybatis/schedule.xml" />
	<!-- 승민 -->	
		<mapper resource="kr/or/ddit/mybatis/member.xml" />
	<!-- 윤희 -->
		<mapper resource="kr/or/ddit/mybatis/board.xml" />
	<!-- 수아 -->
		<mapper resource="kr/or/ddit/mybatis/knowledge.xml" />
		<mapper resource="kr/or/ddit/mybatis/fileitem.xml" />
		<mapper resource="kr/or/ddit/mybatis/healthFile.xml" />
		<mapper resource="kr/or/ddit/mybatis/healthImage.xml" />
	
	</mappers>
</configuration>