<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="iq">

		<insert id="insertIq" parameterType="iqVO">
    		INSERT INTO KNOWLEDGE(IS_NO
									,IS_DATE   
									,COUNT
									)
							VALUES(IQ_STATISTICS_SEQ.nextval
									,sysdate 
									,#{count}  
									)
    	</insert>

</mapper>